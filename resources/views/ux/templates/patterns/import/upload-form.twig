{% extends "patterns/layouts/base.twig" %}

{% block page_header %}
    <h1 class="text-semibold mr-auto text-center">Upload file</h1>
{% endblock %}

{% block content %}
    <div class="row">
        <div class="col-md-6">
            <form>
                <div class="faux-file-field">
                    <div class="faux-file">
                        <button class="btn btn-default">Browse</button>
                        <input type="file" name="import-file" id="import-file" data-mc-el="browseFileInput" class="mc-file-upload">
                    </div>
                </div>
                <input type="submit" class="btn btn-default" value="Upload">
            </form>
        </div>
    </div>
{% endblock %}

